<import name="anu-xnavigator" src="../../../npm/schnee-ui/components/XNavigator/index.ux"></import>
<import name="anu-page-wrapper" src="../../../components/PageWrapper/index.ux"></import>
<template>
    <anu-page-wrapper>
        <div class="anu-block">
            <div class="anu-page-header"><text>{{state.title}}</text></div>
            <block for="(i1301,item) in state.array">
                <div class='anu-item'>
                    <anu-xnavigator data-instance-uid="{{'i49_28_' + i1301}}">{{item.name}}</anu-xnavigator>
                </div>
            </block>
        </div>
    </anu-page-wrapper>
</template>
<script>
import React from "../../../ReactQuick.js";

function P() {
    this.state = {
        title: "内置组件",
        array: "button,checkbox,input,slider,picker,radio,textarea,label,audio,camera,image,video".split(",").map(function(name) {
            return {
                url: "/pages/native" + "/" + name + "/index",
                name: name
            };
        })
    };
}

P = React.toClass(P, React.Component, {
    componentWillMount: function() {
        console.log("native componentWillMount");
    },
    componentDidMount: function() {
        console.log("native componentDidMount");
    },
    gotoSome: function(url) {
        if (url) {
            React.api.navigateTo({
                url
            });
        }
    },
    render: function() {
        var h = React.createElement;
        return h("div", {
            class: "anu-block"
        }, h("div", {
            class: "anu-page-header"
        }, this.state.title), this.state.array.map(function(item, i1301) {
            return h("div", {
                class: "anu-item"
            }, h(React.useComponent, {
                onClick: this.gotoSome.bind(this, item.url),
                "open-type": "navigate",
                "hover-class": "navigator-hover",
                url: item.url,
                is: "XNavigator",
                "data-instance-uid": 'i49_28_' + i1301
            }, item.name));
        }, this));;
    },
    classUid: "c1930"
}, {});
P.config = {
    navigationBarTextStyle: "white",
    navigationBarBackgroundColor: "#0088a4",
    navigationBarTitleText: "内置组件",
    backgroundColor: "orange",
    backgroundTextStyle: "light"
};
P = React.registerPage(P, "pages/native/index/index");
export default P;
</script>
